parameters:
    level: 6
    phpVersion: 80200

    # Dein Modulverzeichnis analysieren
    paths:
        - ./

    # Stubs mit IPS-Funktionen
    scanDirectories:
        - %env.IPS_STUBS_PATH%

    excludePaths:
        analyse:
            - %env.IPS_STUBS_PATH%/*
            
    ignoreErrors:
        - '#Method BlinkHomeClient::OpenAuth\(\) return type has no value type specified in iterable type array#'


    # Supress ignore Errors
    reportUnmatchedIgnoredErrors: false